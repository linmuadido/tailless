#ADDITIONAL="-DTEST_ALIGN=0 -DDUMP_PTR"
ADDITIONAL="-DTEST_ALIGN=16 -DVERIFY_POOL"
#ADDITIONAL="-DTEST_ALIGN=16"
export ADDITIONAL
gg -o tmp.out -DUSE_NEW $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& use_new
gg -o tmp.out -DUSE_MALLOC $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& use_malloc
gg -o tmp.out -DUSE_LOCAL $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& b
gg -o tmp.out $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& c 
gg -o tmp.out -DV2 -DUSE_LOCAL $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& d
gg -o tmp.out -DV2 $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& e 
gg -o tmp.out -DV3 -DUSE_LOCAL $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& f
gg -o tmp.out -DV3 $ADDITIONAL simple_pool_test.cpp -I ../
perf stat -d ./tmp.out >& g 
